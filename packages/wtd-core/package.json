{
  "name": "wtd-core",
  "version": "2.0.0-rc.0",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "npx shx rm -fr dist && npx shx rm -fr tsconfig.tsbuildinfo",
    "doc": "npx shx rm -fr docs && npx typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "lint": "npx eslint src",
    "compile": "tsc -b",
    "watch": "tsc -w",
    "build": "npm run clean && npm run compile"
  },
  "dependencies": {
  },
  "devDependencies": {
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisalmen/three-wtm",
    "directory": "packages/three-wtm"
  },
  "homepage": "https://github.com/kaisalmen/three-wtm/blob/main/packages/wtd-core/README.md",
  "bugs": "https://github.com/kaisalmen/three-wtm/issues",
  "author": {
    "name": "kaisalmen",
    "url": "https://www.kaisalmen.de"
  }
}
