{
  "name": "wtd-examples",
  "version": "2.0.0-beta.0",
  "type": "module",
  "private": "true",
  "scripts": {
    "prepare": "npx shx mkdir -p ./src/worker/imported && npx shx cp -f ./node_modules/wwobjloader2/src/worker/OBJLoader2Worker.js ./src/worker/imported",
    "clean": "shx rm -fr ./dist && shx rm -fr tsconfig.tsbuildinfo",
    "dev": "npm run build:worker && vite",
    "lint": "npx eslint src",
    "compile": "tsc -b",
    "build:worker": "npx shx rm -f ./dist/helloWorldWorkerStandard.js && vite -c vite.config.worker.ts build",
    "build": "npm run clean && npm run build:worker && npm run compile",
    "serve": "tsc && vite preview"
  },
  "dependencies": {
    "wtd-core": "../packages/wtd-core",
    "wtd-three-ext": "../packages/wtd-three-ext",
    "wwobjloader2": "^5.0.0-beta.0",
    "lil-gui": "^0.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisalmen/three-wtm",
    "directory": "packages/three-wtm"
  },
  "homepage": "https://github.com/kaisalmen/three-wtm/blob/main/packages/examples/README.md",
  "bugs": "https://github.com/kaisalmen/three-wtm/issues",
  "author": {
    "name": "kaisalmen",
    "url": "https://www.kaisalmen.de"
  }
}
