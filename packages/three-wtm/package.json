{
  "name": "three-wtm",
  "version": "2.0.0-next.3",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "clean": "shx rm -fr ./dist && shx rm -fr tsconfig.tsbuildinfo",
    "dev": "vite",
    "doc": "jsdoc -c jsdoc.json",
    "lint": "npx eslint src",
    "compile": "tsc -b",
    "watch": "tsc -w",
    "build": "npm run clean && npm run compile",
    "serve": "tsc && vite preview"
  },
  "dependencies": {
    "three": "^0.139.2",
    "@types/three": "^0.139.0"
  },
  "devDependencies": {
    "jsdoc": "^3.6.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisalmen/three-wtm",
    "directory": "packages/three-wtm"
  },
  "homepage": "https://github.com/kaisalmen/three-wtm/blob/main/packages/three-wtm/README.md",
  "bugs": "https://github.com/kaisalmen/three-wtm/issues",
  "author": {
    "name": "kaisalmen",
    "url": "https://www.kaisalmen.de"
  }
}
