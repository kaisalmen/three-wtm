{
  "name": "three-wtm",
  "version": "2.0.0-dev",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "splitting": false,
    "sourcemap": true,
    "dts": true,
    "format": [
      "esm"
    ],
    "target": "esnext",
    "external": [
      "three"
    ]
  },
  "scripts": {
    "clean": "shx rm -fr ./dist",
    "dev": "vite",
    "doc": "jsdoc -c jsdoc.json",
    "lint": "npx eslint src",
    "build": "npm run clean && tsup",
    "serve": "tsc && vite preview"
  },
  "dependencies": {
    "three": "^0.138.3",
    "@types/three": "^0.138.0"
  },
  "devDependencies": {
    "jsdoc": "^3.6.10",
    "tsup": "^5.12.0"
  }
}
