{
  "name": "wtd-three-ext",
  "version": "2.1.0",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./bundle": {
      "types": "./dist/index.d.ts",
      "module": "./bundle/index.js",
      "import": "./bundle/index.js",
      "default": "./bundle/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "dist/index"
      ],
      "bundle": [
        "dist/index"
      ]
    }
  },
  "files": [
    "dist",
    "bundle",
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "npx shx rm -fr dist bundle tsconfig.tsbuildinfo",
    "doc": "npx shx rm -fr docs && npx typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts",
    "lint": "npx eslint src",
    "compile": "tsc -b",
    "watch": "tsc -w",
    "build:bundle": "vite --config vite.bundle.config.ts build",
    "build": "npm run clean && npm run compile && npm run build:bundle"
  },
  "volta": {
    "node": "16.17.1",
    "npm": "8.19.2"
  },
  "dependencies": {
    "wtd-core": "2.1.0",
    "three": "^0.145.0",
    "@types/three": "^0.144.0"
  },
  "peerDependencies": {
    "three": ">= 0.137.5 < 1"
  },
  "devDependencies": {
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaisalmen/three-wtm",
    "directory": "packages/three-wtm"
  },
  "homepage": "https://github.com/kaisalmen/three-wtm/blob/main/packages/wtd-three-ext/README.md",
  "bugs": "https://github.com/kaisalmen/three-wtm/issues",
  "author": {
    "name": "kaisalmen",
    "url": "https://www.kaisalmen.de"
  }
}
